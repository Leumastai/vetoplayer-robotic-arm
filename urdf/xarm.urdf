<robot name="xarm-lite-6">
    <link name="base_link">
        <visual>    
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://ufactory-xarm-lite-6/meshes/link_base.stl" scale="1.0 1.0 1.0"/>
            </geometry>
        </visual>
    </link>

    <link name="link1">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://ufactory-xarm-lite-6/meshes/link1.stl" scale="1.0 1.0 1.0"/>
            </geometry>
        </visual>
    </link>

    <joint name="base_link1_joint" type="continuous">
        <origin xyz="0 0 0.173" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="link1"/>
        <axis xyz="0 0 1"/>
    </joint>

    <link name="link2">
        <visual>
            <origin xyz="0 0 -0.07" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://ufactory-xarm-lite-6/meshes/link2.stl" scale="1.0 1.0 1.0"/>
            </geometry>
        </visual>
    </link>

    <joint name="link1_link2_joint" type="revolute">
        <origin xyz="0 0.07 0" rpy="-1.6 -1.6 0.0"/>
        <parent link="link1"/>
        <child link="link2"/>
        <axis xyz="0 0 1" />
        <limit lower="-2.02" upper="2.02" velocity="0.0" effort="0.0" />
    </joint>

    <link name="link3">
        <visual>
            <!-- <origin xyz="0.19 0 -0.1" rpy="3 0.0 1.5"/> -->
            <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://ufactory-xarm-lite-6/meshes/link3.stl" scale="1.0 1.0 1.0"/>
            </geometry>
        </visual>
    </link>

    <joint name="link2_link3_joint" type="revolute">
        <origin xyz="0.2 0 -0.07" rpy="3.15 0.0 1.55"/>
        <parent link="link2"/>
        <child link="link3"/>
        <axis xyz="0 0 1"/>
        <limit lower="-0.06" upper="5.24" effort="0.0" velocity="0.0"/>
    </joint>

    <link name="link4">
        <visual>
            <origin xyz="0 0.0001 0.18" rpy="0 0 -0.0001"/>
            <geometry>
                <mesh filename="package://ufactory-xarm-lite-6/meshes/link4.stl" scale="1.0 1.0 1.0"/>
            </geometry>
        </visual>
    </link>

    <joint name="link3_link4_joint" type="continuous">
        <!-- <origin xyz="0.1 -0.03 0.1" rpy="1.54 6.5 0.0"/> -->
        <origin xyz="0.09 -0.03 0.008" rpy="1.5 6.4 0"/>
        <parent link="link3"/>
        <child link="link4"/>
        <axis xyz="0 0 1" />
    </joint>

    <link name="link5">
        <visual>
            <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://ufactory-xarm-lite-6/meshes/link5.stl" scale="1.0 1.0 1.0"/>
            </geometry>
        </visual>
    </link>

    <joint name="link4_link5_joint" type="revolute">
        <origin xyz="-0.01 -0.039 0.18" rpy="1.6 0.0 0.1"/>
        <parent link="link4"/>
        <child link="link5"/>
        <axis xyz="0 0 1" />
        <limit lower="-2.16" upper="2.16" velocity="0.0" effort="0.0" />
    </joint>

    <link name="link6">
        <visual>
            <origin xyz="0 0 0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://ufactory-xarm-lite-6/meshes/link6.stl" scale="1.0 1.0 1.0"/>
            </geometry>
        </visual>
    </link>

    <joint name="link5_link6_joint" type="fixed">
        <origin xyz="0 0.04 -0.001" rpy="1.5 -3.1 0"/>
        <parent link="link5"/>
        <child link="link6"/>
        <axis xyz="0 0 1" />
    </joint>

    <link name="gripper_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <mesh filename="package://ufactory-xarm-lite-6/meshes/gripper_lite.stl" scale="1.0 1.0 1.0"/>
            </geometry>
        </visual>
    </link>

    <joint name="gripper_link6_joint" type="continuous">
        <origin xyz="0 0 -0.02" rpy="3.2 0.0 0.0"/>
        <parent link="link6"/>
        <child link="gripper_link"/>
        <axis xyz="0 0 1" />
    </joint>
    

</robot>
